# GitHub pages config: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
charset = utf8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = tab

[*.md]
trim_trailing_whitespace = false

[*.coffee]
indent_style = tab
indent_size = 3

[{*.json,*.yml,.jshintrc}]
indent_style = space
indent_size = 2
